   "source": [
    "import numpy as np\n",
    "from sympy import limit, Symbol, oo"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 65,
   "metadata": {},
   "outputs": [],
   "source": [
    "def stFm(n):\n",
    "    return ((2*np.pi*n)**1/2)*(n/np.e)**n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 66,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
